{
  "name": "bivee.co",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Bivee, Inc.",
  "bugs": {
    "url": "https://github.com/biveeco/bvSite/issues"
  },
  "description": "Refresh of the Bivee website",
  "homepage": "https://github.com/biveeco/bvSite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biveeco/bvSite.git"
  },
  "config": {
    "css_src": "source/assets/stylesheets",
    "css_bundle": "source/assets/dist/stylesheets",
    "js_src": "source/assets/dist/javascripts",
    "icons_src": "./source/assets/images/icons",
    "icons_dest": "./source/components"
  },
  "scripts": {
    "javascripts:build": "webpack -p --bail --",
    "javascripts:watch": "webpack --watch -d --",
    "icons": "svg-sprite --symbol --symbol-dest $npm_package_config_icons_dest --symbol-sprite 'spritemap.erb' --symbol-inline --symbol-prefix 'icon-%s' $npm_package_config_icons_src/**/*.svg",
    "middleman": "bundle exec middleman build --",
    "dev": "yarn run build && yarn run javascripts:watch",
    "build": "yarn run javascripts:build",
    "predeploy": "yarn run middleman"
  },
  "dependencies": {
    "SassyLists": "git+https://github.com/at-import/SassyLists.git",
    "bourbon": "*",
    "clipboard": "^1.5.12",
    "include-media": "*",
    "modularscale-sass": "*",
    "normalize.css": "*",
    "picturefill": "*"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "clean-webpack-plugin": "^0.1.6",
    "svg-sprite": "^1.3.1",
    "svgo": "^0.6.6",
    "webpack": "^1.12.10"
  }
}
